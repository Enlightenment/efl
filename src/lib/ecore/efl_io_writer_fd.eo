mixin Efl.Io.Writer_Fd extends Efl.Io.Writer {
    [[Writes fd using write(2).

      @since 1.19
    ]]

    methods {
        @property writer_fd {
            [[Writer file descriptor]]
            get {}
            set @protected {}
            values {
                fd: int; [[File descriptor]]
            }
        }
    }

    implements {
        Efl.Io.Writer.write;
        Efl.Io.Writer.can_write { get; set; }
    }
}
