            visible: 0;
         }
      }
      program {
         signal: "efl,action,show,vbar"; source: "efl";
         action: STATE_SET "default" 0.0;
         target: "sb_vbar";
         target: "sb_vbar_show";
         target: "efl.dragable.vbar";
