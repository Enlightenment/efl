         part { name: "clo";
            mouse_events: 0;
            clip_to: "clip";
            description { state: "default" 0.0;
               fill {
                  smooth: 0;
                  origin {
                     relative: 0.0 0.0;
                     offset: 0 0;
                  }
                  size {
                     relative: 1.84722 1.0;
                     offset: 0 0;
                  }
