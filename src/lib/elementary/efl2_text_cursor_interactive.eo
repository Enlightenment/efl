import eina_types;

class @beta Efl2.Text.Cursor_Interactive extends Efl2.Text.Cursor {
   [[Cursor API
   ]]
   c_prefix: efl_text_cursor;
   methods {
      user_text_insert {
         [[Emulates a user text insertion
         ]]
         params {
            @in text: string; [[Text to append (UTF-8 format).]]
         }
         return: int; [[Length of the appended text.]]
      }
   }
   events {
      // FIXME: This is actually called externally to the cursor and in addition to changed
      changed,user: void; [[Called when cursor changed beause of user interaction]]
   }
}
