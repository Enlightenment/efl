	 part { name: "shad";
	    mouse_events: 0;
	    description { state: "default" 0.0;
	       rel1.offset: -4 -4;
	       rel1.to: "base";
	       rel2.offset: 3 3;
	       rel2.to: "base";
	       image {
                  normal: "flip_base_shad.png";
		  border: 8 8 8 8;
	       }
	    }
	 }
	 part { name: "base";
	    scale: 1;
	    description { state: "default" 0.0;
	       rel1.offset: 4 4;
	       rel2.offset: -5 -5;
	       // FIXME 48x96 should be the native pixel design, right now
	       // its 80x160. fix int he artwork later
	       min: 24 48;
               aspect: 0.5 0.5;
