#ifdef GL_ES
#ifdef GL_FRAGMENT_PRECISION_HIGH
precision highp float;
#else
precision mediump float;
#endif
#endif
attribute vec4 vertex;
attribute vec4 color;
attribute vec2 tex_coord, tex_coordm;
uniform mat4 mvp;
varying vec4 col;
varying vec2 tex_c, tex_cm;
void main()
{
   gl_Position = mvp * vertex;
   col = color;
   tex_c = tex_coord;
   tex_cm = tex_coordm;
}
