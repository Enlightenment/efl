   data.item: "treesize" TREEPAD;
   data.item: "flips" "elm.text.flip";
   inherit: "genlist_base";
   parts {
      //##//
      text { "elm.text";
         effect: SHADOW BOTTOM;
         scale;
         desc { "default";
            rel1.offset: 2 3;
